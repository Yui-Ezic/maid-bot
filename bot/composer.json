{
  "name": "yui-ezic/maid-bot",
  "type": "project",
  "license": "MIT",
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "thecodingmachine/discovery": true
    }
  },
  "require": {
    "php": "^8.0",
    "laminas/laminas-config-aggregator": "^1.7",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.4",
    "slim/slim": "^4.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "files": [
      "src/env.php"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
